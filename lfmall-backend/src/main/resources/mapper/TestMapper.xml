<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lfmall.backend.test.mapper.TestMapper">

    <select id="selectHealth" resultType="com.lfmall.backend.test.dto.TestDto">
        SELECT
            id,
            message
        FROM test_health
        LIMIT 1
    </select>

</mapper>